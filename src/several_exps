#!/bin/bash
#
# PROCESS=(True False)
DATASETS=(mnist-test usps mnist fashion_mnist)
ITER=(0 1 2 3 4)
#
for d in "${DATASETS[@]}";
do
  for i in "${ITER[@]}";
  do
    # python baseline_clustering.py --method dbscan --dataset_name $d --verbose 0 --random_seed $i
    CUDA_VISIBLE_DEVICES=6 python unsupervised_clustering.py --dataset_name $d --verbose 1 --random_seed $i --num_candidates 9000
      # --poc_preprocessing $p
  done
done
